<% layout("/layouts/boilerplate")%>


<div class="row mt-4">
  <div class="alert alert-danger col-4 mx-auto d-flex align-items-center py-2 px-3 shadow-sm rounded-3" role="alert" style="font-size: 0.9rem;">
    <svg class="bi flex-shrink-0 me-2" width="18" height="18" role="img" aria-label="Danger:">
      <use xlink:href="#exclamation-triangle-fill"/>
    </svg>
    <span class="fw-semibold"><%= err.message %></span>
    
  </div>
</div>
